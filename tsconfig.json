{
        "compilerOptions": {
          "target": "es2020",
          "module": "commonjs",
          "outDir": "./dist",
          "rootDir": "./src",
          "strict": true,
          "esModuleInterop": true,
          "skipLibCheck": true,
          "forceConsistentCasingInFileNames": true
        },
        "include": ["src/**/*.ts", "src/**/*.d.ts", "env.d.ts"],
        "exclude": [
          "node_modules",
          "dist"
        ]
      }
      